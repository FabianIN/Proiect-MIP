<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="340.0"
	prefWidth="480.0" style="-fx-background-color: #cff2fe;"
	xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
	fx:controller="controllers.ControllerRemoveGame">
	<children>
		<VBox prefHeight="340.0" prefWidth="100.0"
			style="-fx-background-color: #68d7fb;" AnchorPane.bottomAnchor="0.0"
			AnchorPane.topAnchor="0.0" />
		<ImageView fitHeight="189.0" fitWidth="166.0" layoutX="14.0"
			layoutY="63.0" pickOnBounds="true" preserveRatio="true">
			<image>
				<Image url="@../../bin/resourceView/logo.png" />
			</image>
		</ImageView>
		<ImageView fitHeight="60.0" fitWidth="438.0" layoutX="22.0"
			layoutY="14.0" pickOnBounds="true" preserveRatio="true">
			<image>
				<Image url="@../../bin/resourceView/text.png" />
			</image>
		</ImageView>
		<Button fx:id="btnReset" layoutX="323.0" layoutY="283.0"
			mnemonicParsing="false" onAction="#reset" text="Reset"
			textAlignment="CENTER" textFill="#6e2605"
			AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="290.0">
			<font>
				<Font name="Broadway" size="14.0" />
			</font>
		</Button>
		<Button fx:id="btnDelete" layoutX="300.0" layoutY="283.0"
			mnemonicParsing="false" onAction="#delete" text="Delete"
			textAlignment="CENTER" textFill="#6e2605"
			AnchorPane.topAnchor="290.0">
			<font>
				<Font name="Broadway" size="14.0" />
			</font>
		</Button>
		<Label alignment="TOP_LEFT" layoutX="192.0" layoutY="153.0"
			text="Game ID" textFill="#6e2605" AnchorPane.rightAnchor="195.0"
			AnchorPane.topAnchor="155.0">
			<font>
				<Font name="Broadway" size="14.0" />
			</font>
		</Label>
		<TextField fx:id="txtid" layoutX="303.0" layoutY="146.0"
			promptText="enter game ID" AnchorPane.rightAnchor="30.0"
			AnchorPane.topAnchor="150.0" />
	</children>
</AnchorPane>
